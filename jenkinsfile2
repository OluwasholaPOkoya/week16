pipeline {
    agent any
    tools{
        maven 'M2_HOME'
    }
     stage('sonarqube scan'){
            stage{
                sh 'mvn sonar:sonar'
            }
        }
    stages{
        stage('all maven command'){
            steps{
                sh 'mvn clean compile test install package'
            }
        }
            
        
    }
}